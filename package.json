{
  "name": "iptv-search-vue",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/api/program.js",
  "engines": {
    "node": "7.x"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:ui": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev:api": "ts-node-dev --transpileOnly --respawn --project ./api/tsconfig.json ./api/src/program.ts",
    "build:api": "tsc -p ./api/tsconfig.json",
    "serve:api": "tsc -p ./api/tsconfig.json && nodemon ./dist/api/program.js",
    "start": "npm run build:api && npm run build:ui && node ./dist/api/program.js"
  },
  "dependencies": {
    "@types/node": "^12.7.2",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "material-design-icons-iconfont": "^5.0.1",
    "nodemon": "^1.19.1",
    "ts-node-dev": "^1.0.0-pre.41",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuetify": "^2.0.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.18.2",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "typescript": "^3.4.3",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
